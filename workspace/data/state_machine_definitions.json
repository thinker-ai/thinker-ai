{
  "titanic_survival": {
    "is_root": true,
    "description": "Predict passenger survival outcome based on Titanic dataset",
    "states_def": [
      {
        "name": "data_analysis",
        "is_start": true,
        "state_context_class_name": "titanic_survival.data_analysis.contexts.DataAnalysisContext",
        "description": "Perform exploratory data analysis on the Titanic dataset",
        "task_type": "eda",
        "actions": [
          {
            "on_command": "perform_eda",
            "pre_check_list": ["Ensure dataset is loaded", "Ensure necessary libraries are imported"],
            "full_class_name": "titanic_survival.data_analysis.actions.PerformEDAAction",
            "post_check_list": ["EDA report generated", "Insights on data distribution and missing values"]
          }
        ],
        "events": ["eda_completed"]
      },
      {
        "name": "data_preprocessing",
        "is_start": false,
        "state_context_class_name": "titanic_survival.data_preprocessing.contexts.DataPreprocessingContext",
        "description": "Preprocess the Titanic dataset",
        "task_type": "data preprocessing",
        "actions": [
          {
            "on_command": "preprocess_data",
            "pre_check_list": ["EDA completed", "Insights on missing values and data distribution"],
            "full_class_name": "titanic_survival.data_preprocessing.actions.PreprocessDataAction",
            "post_check_list": ["Data cleaned", "Missing values handled", "Data ready for feature engineering"]
          }
        ],
        "events": ["data_preprocessing_completed"]
      },
      {
        "name": "feature_engineering",
        "is_start": false,
        "state_context_class_name": "titanic_survival.feature_engineering.contexts.FeatureEngineeringContext",
        "description": "Create new features for the Titanic dataset",
        "task_type": "feature engineering",
        "actions": [
          {
            "on_command": "engineer_features",
            "pre_check_list": ["Data preprocessed", "Feature engineering plan ready"],
            "full_class_name": "titanic_survival.feature_engineering.actions.EngineerFeaturesAction",
            "post_check_list": ["New features created", "Feature set ready for modeling"]
          }
        ],
        "events": ["feature_engineering_completed"]
      },
      {
        "name": "model_training",
        "is_start": false,
        "state_context_class_name": "titanic_survival.model_training.contexts.ModelTrainingContext",
        "description": "Train a model to predict survival outcome",
        "task_type": "model train",
        "actions": [
          {
            "on_command": "train_model",
            "pre_check_list": ["Feature set ready", "Model training plan ready"],
            "full_class_name": "titanic_survival.model_training.actions.TrainModelAction",
            "post_check_list": ["Model trained", "Training metrics available"]
          }
        ],
        "events": ["model_training_completed"]
      },
      {
        "name": "model_evaluation",
        "is_start": false,
        "state_context_class_name": "titanic_survival.model_evaluation.contexts.ModelEvaluationContext",
        "description": "Evaluate the trained model on evaluation data",
        "task_type": "model evaluate",
        "actions": [
          {
            "on_command": "evaluate_model",
            "pre_check_list": ["Model trained", "Evaluation data ready"],
            "full_class_name": "titanic_survival.model_evaluation.actions.EvaluateModelAction",
            "post_check_list": ["Model evaluated", "Evaluation metrics available"]
          }
        ],
        "events": ["model_evaluation_completed"]
      },
      {
        "name": "end_state_success",
        "description": "Successfully completed the Titanic survival prediction task",
        "state_context_class_name": "titanic_survival.end_state_success.contexts.EndStateSuccessContext"
      },
      {
        "name": "end_state_failure",
        "description": "Failed to complete the Titanic survival prediction task",
        "state_context_class_name": "titanic_survival.end_state_failure.contexts.EndStateFailureContext"
      }
    ],
    "transitions": [
      {
        "event": "eda_completed",
        "source": "data_analysis",
        "target": "data_preprocessing"
      },
      {
        "event": "data_preprocessing_completed",
        "source": "data_preprocessing",
        "target": "feature_engineering"
      },
      {
        "event": "feature_engineering_completed",
        "source": "feature_engineering",
        "target": "model_training"
      },
      {
        "event": "model_training_completed",
        "source": "model_training",
        "target": "model_evaluation"
      },
      {
        "event": "model_evaluation_completed",
        "source": "model_evaluation",
        "target": "end_state_success"
      }
    ]
  }
}