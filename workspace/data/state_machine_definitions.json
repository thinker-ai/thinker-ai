{
  "titanic_survival_state_machine": {
    "name": "titanic_survival",
    "description": "Predict passenger survival outcome on the Titanic dataset",
    "states_def": [
      {
        "id": "eda_state",
        "name": "Exploratory Data Analysis",
        "is_start": true,
        "state_context_class_name": "titanic_survival.ExploratoryDataAnalysis.contexts.EDAStateContext",
        "description": "Perform exploratory data analysis on the Titanic dataset",
        "task_type": "eda",
        "actions": [
          {
            "on_command": "perform_eda",
            "full_class_name": "titanic_survival.ExploratoryDataAnalysis.actions.PerformEDAAction"
          }
        ],
        "events": [
          "eda_completed"
        ]
      },
      {
        "id": "data_preprocessing_state",
        "name": "Data Preprocessing",
        "is_start": false,
        "state_context_class_name": "titanic_survival.DataPreprocessing.contexts.DataPreprocessingStateContext",
        "description": "Preprocess the Titanic dataset",
        "task_type": "data preprocessing",
        "actions": [
          {
            "on_command": "preprocess_data",
            "full_class_name": "titanic_survival.DataPreprocessing.actions.PreprocessDataAction"
          }
        ],
        "events": [
          "data_preprocessing_completed"
        ]
      },
      {
        "id": "feature_engineering_state",
        "name": "Feature Engineering",
        "is_start": false,
        "state_context_class_name": "titanic_survival.FeatureEngineering.contexts.FeatureEngineeringStateContext",
        "description": "Perform feature engineering on the Titanic dataset",
        "task_type": "feature engineering",
        "actions": [
          {
            "on_command": "engineer_features",
            "full_class_name": "titanic_survival.FeatureEngineering.actions.EngineerFeaturesAction"
          }
        ],
        "events": [
          "feature_engineering_completed"
        ]
      },
      {
        "id": "model_training_state",
        "name": "Model Training",
        "is_start": false,
        "state_context_class_name": "titanic_survival.ModelTraining.contexts.ModelTrainingStateContext",
        "description": "Train a model to predict survival on the Titanic dataset",
        "task_type": "model train",
        "actions": [
          {
            "on_command": "train_model",
            "full_class_name": "titanic_survival.ModelTraining.actions.TrainModelAction"
          }
        ],
        "events": [
          "model_training_completed"
        ]
      },
      {
        "id": "model_evaluation_state",
        "name": "Model Evaluation",
        "is_start": false,
        "state_context_class_name": "titanic_survival.ModelEvaluation.contexts.ModelEvaluationStateContext",
        "description": "Evaluate the trained model on the evaluation dataset",
        "task_type": "model evaluate",
        "actions": [
          {
            "on_command": "evaluate_model",
            "full_class_name": "titanic_survival.ModelEvaluation.actions.EvaluateModelAction"
          }
        ],
        "events": [
          "model_evaluation_completed"
        ]
      },
      {
        "id": "end_state",
        "name": "End State",
        "description": "End of the state machine",
        "state_context_class_name": "titanic_survival.EndState.contexts.EndStateContext"
      }
    ],
    "transitions": [
      {
        "event": "eda_completed",
        "source": "Exploratory Data Analysis",
        "target": "Data Preprocessing"
      },
      {
        "event": "data_preprocessing_completed",
        "source": "Data Preprocessing",
        "target": "Feature Engineering"
      },
      {
        "event": "feature_engineering_completed",
        "source": "Feature Engineering",
        "target": "Model Training"
      },
      {
        "event": "model_training_completed",
        "source": "Model Training",
        "target": "Model Evaluation"
      },
      {
        "event": "model_evaluation_completed",
        "source": "Model Evaluation",
        "target": "End State"
      }
    ]
  }
}