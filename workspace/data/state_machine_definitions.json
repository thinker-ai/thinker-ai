{
  "titanic_survival_prediction": {
    "description": "Predict passenger survival outcome on the Titanic dataset",
    "states_def": [
      {
        "id": "1",
        "name": "data_analysis",
        "is_start": true,
        "state_context_class_name": "DataAnalysisStateContext",
        "description": "Perform exploratory data analysis",
        "task_type": "eda",
        "actions": [
          {
            "on_command": "perform_eda",
            "full_class_name": "PerformEDAAction"
          }
        ],
        "events": [
          "eda_completed"
        ]
      },
      {
        "id": "2",
        "name": "data_preprocessing",
        "is_start": false,
        "state_context_class_name": "DataPreprocessingStateContext",
        "description": "Preprocess the dataset",
        "task_type": "data preprocessing",
        "actions": [
          {
            "on_command": "preprocess_data",
            "full_class_name": "PreprocessDataAction"
          }
        ],
        "events": [
          "data_preprocessing_completed"
        ]
      },
      {
        "id": "3",
        "name": "feature_engineering",
        "is_start": false,
        "state_context_class_name": "FeatureEngineeringStateContext",
        "description": "Create new features for the dataset",
        "task_type": "feature engineering",
        "actions": [
          {
            "on_command": "engineer_features",
            "full_class_name": "EngineerFeaturesAction"
          }
        ],
        "events": [
          "feature_engineering_completed"
        ]
      },
      {
        "id": "4",
        "name": "model_training",
        "is_start": false,
        "state_context_class_name": "ModelTrainingStateContext",
        "description": "Train the machine learning model",
        "task_type": "model train",
        "actions": [
          {
            "on_command": "train_model",
            "full_class_name": "TrainModelAction"
          }
        ],
        "events": [
          "model_training_completed"
        ]
      },
      {
        "id": "5",
        "name": "model_evaluation",
        "is_start": false,
        "state_context_class_name": "ModelEvaluationStateContext",
        "description": "Evaluate the trained model",
        "task_type": "model evaluate",
        "actions": [
          {
            "on_command": "evaluate_model",
            "full_class_name": "EvaluateModelAction"
          }
        ],
        "events": [
          "model_evaluation_completed"
        ]
      },
      {
        "id": "6",
        "name": "end_state",
        "description": "End state of the state machine",
        "state_context_class_name": "EndStateContext"
      }
    ],
    "transitions": [
      {
        "event": "eda_completed",
        "source": "data_analysis",
        "target": "data_preprocessing"
      },
      {
        "event": "data_preprocessing_completed",
        "source": "data_preprocessing",
        "target": "feature_engineering"
      },
      {
        "event": "feature_engineering_completed",
        "source": "feature_engineering",
        "target": "model_training"
      },
      {
        "event": "model_training_completed",
        "source": "model_training",
        "target": "model_evaluation"
      },
      {
        "event": "model_evaluation_completed",
        "source": "model_evaluation",
        "target": "end_state"
      }
    ]
  }
}