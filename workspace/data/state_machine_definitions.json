{
  "titanic_survival": {
    "titanic_survival": {
      "is_root": true,
      "description": "Predict passenger survival outcome based on Titanic dataset",
      "state_context_builder_full_class_name": "titanic_survival.TitanicSurvivalStateContextBuilder",
      "states_def": [
        {
          "name": "data_analysis",
          "is_start": true,
          "state_context_class_name": "titanic_survival.data_analysis.contexts.DataAnalysisStateContext",
          "description": "Perform exploratory data analysis",
          "task_type": "eda",
          "actions": [
            {
              "on_command": "perform_eda",
              "pre_check_list": ["Ensure data is loaded", "Check for necessary libraries"],
              "full_class_name": "titanic_survival.data_analysis.actions.PerformEDAAction",
              "post_check_list": ["EDA results are generated"]
            }
          ],
          "events": [
            "perform_eda_result_success",
            "perform_eda_result_failure"
          ]
        },
        {
          "name": "data_preprocessing",
          "is_start": false,
          "state_context_class_name": "titanic_survival.data_preprocessing.contexts.DataPreprocessingStateContext",
          "description": "Preprocess the dataset",
          "task_type": "data preprocessing",
          "actions": [
            {
              "on_command": "preprocess_data",
              "pre_check_list": ["EDA results are available"],
              "full_class_name": "titanic_survival.data_preprocessing.actions.PreprocessDataAction",
              "post_check_list": ["Data is preprocessed"]
            }
          ],
          "events": [
            "preprocess_data_result_success",
            "preprocess_data_result_failure"
          ]
        },
        {
          "name": "feature_engineering",
          "is_start": false,
          "state_context_class_name": "titanic_survival.feature_engineering.contexts.FeatureEngineeringStateContext",
          "description": "Create new features for the dataset",
          "task_type": "feature engineering",
          "actions": [
            {
              "on_command": "engineer_features",
              "pre_check_list": ["Data is preprocessed"],
              "full_class_name": "titanic_survival.feature_engineering.actions.EngineerFeaturesAction",
              "post_check_list": ["Features are engineered"]
            }
          ],
          "events": [
            "engineer_features_result_success",
            "engineer_features_result_failure"
          ]
        },
        {
          "name": "model_training",
          "is_start": false,
          "state_context_class_name": "titanic_survival.model_training.contexts.ModelTrainingStateContext",
          "description": "Train the model",
          "task_type": "model train",
          "actions": [
            {
              "on_command": "train_model",
              "pre_check_list": ["Features are engineered"],
              "full_class_name": "titanic_survival.model_training.actions.TrainModelAction",
              "post_check_list": ["Model is trained"]
            }
          ],
          "events": [
            "train_model_result_success",
            "train_model_result_failure"
          ]
        },
        {
          "name": "model_evaluation",
          "is_start": false,
          "state_context_class_name": "titanic_survival.model_evaluation.contexts.ModelEvaluationStateContext",
          "description": "Evaluate the model",
          "task_type": "model evaluate",
          "actions": [
            {
              "on_command": "evaluate_model",
              "pre_check_list": ["Model is trained"],
              "full_class_name": "titanic_survival.model_evaluation.actions.EvaluateModelAction",
              "post_check_list": ["Model evaluation results are available"]
            }
          ],
          "events": [
            "evaluate_model_result_success",
            "evaluate_model_result_failure"
          ]
        },
        {
          "name": "end_state_success",
          "description": "End state for successful completion",
          "state_context_class_name": "titanic_survival.end_state.contexts.EndStateSuccessContext"
        },
        {
          "name": "end_state_failure",
          "description": "End state for failure",
          "state_context_class_name": "titanic_survival.end_state.contexts.EndStateFailureContext"
        }
      ],
      "transitions": [
        {
          "event": "perform_eda_result_success",
          "source": "data_analysis",
          "target": "data_preprocessing"
        },
        {
          "event": "perform_eda_result_failure",
          "source": "data_analysis",
          "target": "end_state_failure"
        },
        {
          "event": "preprocess_data_result_success",
          "source": "data_preprocessing",
          "target": "feature_engineering"
        },
        {
          "event": "preprocess_data_result_failure",
          "source": "data_preprocessing",
          "target": "end_state_failure"
        },
        {
          "event": "engineer_features_result_success",
          "source": "feature_engineering",
          "target": "model_training"
        },
        {
          "event": "engineer_features_result_failure",
          "source": "feature_engineering",
          "target": "end_state_failure"
        },
        {
          "event": "train_model_result_success",
          "source": "model_training",
          "target": "model_evaluation"
        },
        {
          "event": "train_model_result_failure",
          "source": "model_training",
          "target": "end_state_failure"
        },
        {
          "event": "evaluate_model_result_success",
          "source": "model_evaluation",
          "target": "end_state_success"
        },
        {
          "event": "evaluate_model_result_failure",
          "source": "model_evaluation",
          "target": "end_state_failure"
        }
      ]
    }
  }
}
